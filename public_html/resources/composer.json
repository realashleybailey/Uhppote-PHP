{
    "$schema": "https://raw.githubusercontent.com/composer/composer/master/res/composer-schema.json",
    "name": "realashleybailey/uhppote-php",
    "description": "Uhppote PHP interface",
    "type": "project",
    "license": "GPL",
    "require": {
        "vlucas/phpdotenv": "^5.3",
        "firebase/php-jwt": "^5.4",
        "elegantweb/sanitizer": "^1.1"
    },
    "autoload": {
        "classmap": [
            "extension/*",
            "src/",
            "modules/"
        ],
        "files": [
            "library/templateFunctions.php",
            "library/viewFunctions.php"
        ]
    }
}